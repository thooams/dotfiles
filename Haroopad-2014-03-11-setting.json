{
  "Code": {
    "theme": "solarized_light",
    "displayLineNumber": false
  },
  "Editor": {
    "theme": "solarized dark",
    "displayLineNumber": true,
    "vimKeyBinding": true,
    "autoPairCharacters": true,
    "indentWithTabs": false,
    "displayActiveLine": true,
    "fontSize": 13,
    "userTheme": "default"
  },
  "General": {
    "enableSyncScroll": true,
    "playKeypressSound": false,
    "enableAutoComplete": true,
    "enableLastFileRestore": true,
    "displayLanguage": "fr-fr",
    "dateFormat": "ll"
  },
  "Markdown": {
    "tables": true,
    "breaks": true,
    "sanitize": false,
    "smartLists": true,
    "smartypants": true,
    "mathjax": true,
    "dollarSign": true
  },
  "Viewer": {
    "theme": "haroopad",
    "clickableLink": true,
    "fontSize": 15,
    "userTheme": "default"
  },
  "_version": "0.11.1"
}